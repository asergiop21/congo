<p id="notice"><%= notice %></p>

<h1>Bots</h1>

<table>
  <thead>
    <tr>
      <th>Publish</th>
      <th>Code</th>
      <th>Type</th>
      <th>Modality</th>
      <th>Properties</th>
      <th>Region</th>
      <th>Comune</th>
      <th>Street</th>
      <th>Number</th>
      <th>Furnished</th>
      <th>Apt</th>
      <th>Floor</th>
      <th>Bedroom</th>
      <th>Bathroom</th>
      <th>Parkink lo</th>
      <th>Cellar</th>
      <th>Surface</th>
      <th>Surface t</th>
      <th>Price</th>
      <th>Price uf</th>
      <th>Price usd</th>
      <th>Owner</th>
      <th>Phone</th>
      <th>Email</th>
      <th>Bimester</th>
      <th>The geom</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bots.each do |bot| %>
      <tr>
        <td><%= bot.publish %></td>
        <td><%= bot.code %></td>
        <td><%= bot.type %></td>
        <td><%= bot.modality %></td>
        <td><%= bot.properties %></td>
        <td><%= bot.region %></td>
        <td><%= bot.comune %></td>
        <td><%= bot.street %></td>
        <td><%= bot.number %></td>
        <td><%= bot.furnished %></td>
        <td><%= bot.apt %></td>
        <td><%= bot.floor %></td>
        <td><%= bot.bedroom %></td>
        <td><%= bot.bathroom %></td>
        <td><%= bot.parkink_lo %></td>
        <td><%= bot.cellar %></td>
        <td><%= bot.surface %></td>
        <td><%= bot.surface_t %></td>
        <td><%= bot.price %></td>
        <td><%= bot.price_uf %></td>
        <td><%= bot.price_usd %></td>
        <td><%= bot.owner %></td>
        <td><%= bot.phone %></td>
        <td><%= bot.email %></td>
        <td><%= bot.bimester %></td>
        <td><%= bot.the_geom %></td>
        <td><%= link_to 'Show', bot %></td>
        <td><%= link_to 'Edit', edit_bot_path(bot) %></td>
        <td><%= link_to 'Destroy', bot, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

