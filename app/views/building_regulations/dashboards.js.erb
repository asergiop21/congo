Congo.dashboards.config.layer_type = 'building_regulations_info';
widget =  Congo.dashboards.config.widget;
switch (widget) {
  case 'building_regulations_max_density':
    Congo.building_regulations.config.legends = []
    Congo.dashboards.config.style_layer= 'building_regulations_max_density';
    Congo.building_regulations.config.legends.push({'name':'Menor a 400', 'color':'#f6eff7'});
    Congo.building_regulations.config.legends.push({'name':'400 - 599 ', 'color':'#d0d1e6'});
    Congo.building_regulations.config.legends.push({'name':'600 - 799', 'color':'#a6bddb'});
    Congo.building_regulations.config.legends.push({'name':'800 - 1199', 'color':'#67a9cf'});
    Congo.building_regulations.config.legends.push({'name':'Mayor 1200', 'color':'#1c9099'});
    Congo.map_utils.counties();
    $('#layer-name').text('NORMATIVA - Densidad Máxima');

    break;
  case 'building_regulations_floors':
    Congo.building_regulations.config.legends = []
    Congo.dashboards.config.style_layer = 'building_regulations_floors';
    Congo.building_regulations.config.legends.push({'name':'Menor a 3 Pisos', 'color':'#f6eff7'});
    Congo.building_regulations.config.legends.push({'name':'4 Pisos', 'color':'#d0d1e6'});
    Congo.building_regulations.config.legends.push({'name':'5 - 8 Pisos', 'color':'#a6bddb'});
    Congo.building_regulations.config.legends.push({'name':'9 - 15 Pisos', 'color':'#67a9cf'});
    Congo.building_regulations.config.legends.push({'name':'Mayor 15', 'color':'#1c9099'});
    Congo.map_utils.counties();
    $('#layer-name').text('NORMATIVA - Altura Máxima');
    break;
  default:
    Congo.building_regulations.config.legends = []
    Congo.dashboards.config.style_layer= 'building_regulations_max_density';
    Congo.building_regulations.config.legends.push({'name':'Menor a 400', 'color':'#f6eff7'});
    Congo.building_regulations.config.legends.push({'name':'400 - 599 ', 'color':'#d0d1e6'});
    Congo.building_regulations.config.legends.push({'name':'600 - 799', 'color':'#a6bddb'});
    Congo.building_regulations.config.legends.push({'name':'800 - 1199', 'color':'#67a9cf'});
    Congo.building_regulations.config.legends.push({'name':'Mayor 1200', 'color':'#1c9099'});
    Congo.map_utils.counties();
    $('#layer-name').text('NORMATIVA - Densidad Máxima');
    break;
}
