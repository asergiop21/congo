<p id="notice"><%= notice %></p>

<h1>Future Projects</h1>

<table>
  <thead>
    <tr>
      <th>Code</th>
      <th>Address</th>
      <th>Name</th>
      <th>Role number</th>
      <th>File number</th>
      <th>File data</th>
      <th>Owner</th>
      <th>Legal agent</th>
      <th>Architect</th>
      <th>Floors</th>
      <th>Undergrounds</th>
      <th>Total units</th>
      <th>Total parking</th>
      <th>Total commercials</th>
      <th>M2 approved</th>
      <th>M2 built</th>
      <th>M2 field</th>
      <th>Cadastral date</th>
      <th>Comments</th>
      <th>Bimester</th>
      <th>Year</th>
      <th>Cadastre</th>
      <th>Active</th>
      <th>Project type</th>
      <th>Future project type</th>
      <th>County</th>
      <th>The geom</th>
      <th>T ofi</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @future_projects.each do |future_project| %>
      <tr>
        <td><%= future_project.code %></td>
        <td><%= future_project.address %></td>
        <td><%= future_project.name %></td>
        <td><%= future_project.role_number %></td>
        <td><%= future_project.file_number %></td>
        <td><%= future_project.file_data %></td>
        <td><%= future_project.owner %></td>
        <td><%= future_project.legal_agent %></td>
        <td><%= future_project.architect %></td>
        <td><%= future_project.floors %></td>
        <td><%= future_project.undergrounds %></td>
        <td><%= future_project.total_units %></td>
        <td><%= future_project.total_parking %></td>
        <td><%= future_project.total_commercials %></td>
        <td><%= future_project.m2_approved %></td>
        <td><%= future_project.m2_built %></td>
        <td><%= future_project.m2_field %></td>
        <td><%= future_project.cadastral_date %></td>
        <td><%= future_project.comments %></td>
        <td><%= future_project.bimester %></td>
        <td><%= future_project.year %></td>
        <td><%= future_project.cadastre %></td>
        <td><%= future_project.active %></td>
        <td><%= future_project.project_type %></td>
        <td><%= future_project.future_project_type %></td>
        <td><%= future_project.county %></td>
        <td><%= future_project.the_geom %></td>
        <td><%= future_project.t_ofi %></td>
        <td><%= link_to 'Show', future_project %></td>
        <td><%= link_to 'Edit', edit_future_project_path(future_project) %></td>
        <td><%= link_to 'Destroy', future_project, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Future Project', new_future_project_path %>
