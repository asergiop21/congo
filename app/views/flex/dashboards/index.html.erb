<div class="bg-blue flex-dashboard">
  <div class="container-fluid">
    <div class="row pt-1">
      <div class="col-md-3 p-0 map-box">
        <div class="m-1">
          <div id="map_flex"></div>
        </div>
      </div>
      <div class="col-md-3 p-0 bg-1 d-flex justify-content-center align-items-center" id="intro">
        <div class="bg-1 m-1">
          <div class="container">
            <p>Seleccione en el mapa</p>
          </div>
        </div>
      </div>
      <div class="col-md-3 p-0 d-none select-box" id="select-box">
        <div class="bg-1 m-1">
          <div class="container">
            <div class="row pt-3 d-flex align-items-center">
              <div class="col-6 my-2 text-center">
                <div class="form-group">
                  <p>Tipo de propiedad</p>
                  <select class="form-control" id="prop_type" multiple></select>
                </div>
              </div>
              <div class="col-6 my-2 text-center">
                <div class="form-group">
                  <p>Vendedor</p>
                  <select class="form-control" id="seller_type" multiple></select>
                </div>
              </div>
              <div class="col-12 my-2 text-center">
                <p class="mb-0">Fecha de inscripción</p>
                <input type="range" class="js-range-slider w-100" id="insc_date">
              </div>
              <!--<div class="col-8 my-2 text-center">
                <p>Fecha de inscripción</p>
              </div>-->

              <div class="col-12 my-2 text-center">
                <p class="mb-0">Zona normativa</p>
                <input type="range" class="js-range-slider w-100" id="zone">
              </div>
              <!--<div class="col-8 my-2 text-center">
                <p>Zona normativa</p>
              </div>-->
              <div class="col-12 my-2 text-center">
                <p class="mb-0">Superficie</p>
                <input type="range" class="js-range-slider w-100" id="surface">
              </div>
              <!--<div class="col-8 my-2 text-center">
                <p>Superficie</p>
              </div>-->
              <div class="col-12 my-2 text-center">
                <p class="mb-0">Superficie terreno</p>
                <input type="range" class="js-range-slider w-100" id="t_surface">
              </div>
              <!--<div class="col-8 my-2 text-center">
                <p>Superficie terreno</p>
              </div>-->
              <div class="col-12 my-2 text-center">
                <p class="mb-0">Precio</p>
                <input type="range" class="js-range-slider w-100" id="price">
              </div>
              <!--<div class="col-8 my-2 text-center">
                <p>Precio</p>
              </div>-->
              <div class="col-12 my-2 text-center">
                <p class="mb-0">Precio unitario</p>
                <input type="range" class="js-range-slider w-100" id="u_price">
              </div>
              <div class="col-12 my-2 text-center">
                <button class="btn btn-secondary" onclick="getFilteredData();">Filtrar</button>
              </div>
              <!--<div class="col-8 my-2 text-center">
                <p>Precio unitario</p>
              </div>-->
            </div>
          </div>
        </div>
      </div>
      <div class="col-md-9 table-box">
        <div class="mt-1">
          <div class="container p-0">
            <table id="table" class="table table-bordered">
              <thead class="bg-table-head">
              <tr>
                <th scope="col"><small>N</small></th>
                <th scope="col"><small>Tipo</small></th>
                <th scope="col"><small>Dirección</small></th>
                <th scope="col"><small>Comuna</small></th>
                <th scope="col"><small>Rol</small></th>
                <th scope="col"><small>Foja</small></th>
                <th scope="col"><small>#</small></th>
                <th scope="col"><small>Fecha</small></th>
                <th scope="col"><small>Vendedor</small></th>
                <th scope="col"><small>Util(m2)</small></th>
                <th scope="col"><small>Terreno</small></th>
                <th scope="col"><small>#E</small></th>
                <th scope="col"><small>#B</small></th>
                <th scope="col"><small>UF</small></th>
                <th scope="col"><small>UF(m2)</small></th>
                <th scope="col"><small>Año</small></th>
              </tr>
              </thead>
              <tbody>
              <tr>
                <td colspan="16" class="w-100 text-center"><b>DATOS DE MI PROPIEDAD</b></td>
              </tr>
              </tbody>
            </table>
            <div class="my-4">
              <input type="text" class="user_data" placeholder="Tipo" name="tipo">
              <input type="text" class="user_data" placeholder="Dirección" name="direccion">
              <input type="text" class="user_data" placeholder="Comuna" name="comuna">
              <input type="text" class="user_data" placeholder="Rol" name="rol">
              <input type="text" class="user_data" placeholder="Foja" name="foja">
              <input type="text" class="user_data" placeholder="Fecha" name="fecha">
              <input type="text" class="user_data" placeholder="Vendedor" name="vendedor">
              <input type="text" class="user_data" placeholder="Util(m2)" name="util(m2)">
              <input type="text" class="user_data" placeholder="Terreno" name="terreno">
              <input type="text" class="user_data" placeholder="#E" name="#e">
              <input type="text" class="user_data" placeholder="#B" name="#b">
              <input type="text" class="user_data" placeholder="UF" name="uf">
              <input type="text" class="user_data" placeholder="UF(m2)" name="uf(m2)">
              <input type="text" class="user_data" placeholder="Año" name="año"> <br>
            </div>
            <div class="mb-5">
              <button class="btn btn-secondary" onclick="genCharts();">Generar</button>
              <button class="btn btn-success" id="exportToExcel">Descargar</button>
              <button onclick="clearTable();" class="btn btn-danger">Limpiar</button>
            </div>
          </div>
        </div>
      </div>
    </div>
    <div class="row pt-1 bg-white">
      <div class="col-12">
        <div id="carouselExampleControls" class="carousel slide" data-ride="false" data-interval="false">
          <div class="carousel-inner">
            <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
              <span class="carousel-control-prev-icon" aria-hidden="true"></span>
              <span class="sr-only">Previous</span>
            </a>
            <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
              <span class="carousel-control-next-icon" aria-hidden="true"></span>
              <span class="sr-only">Next</span>
            </a>
            <div class="carousel-item active">
              <div class="row">
                <div class="col-4">
                  <p class="text-center bg-dark text-white">Cantidad</p>
                  <canvas id="chartCantidad"></canvas>
                </div>
                <div class="col-4">
                  <p class="text-center bg-dark text-white">Superficie</p>
                  <canvas id="chartSupUtil"></canvas>
                </div>
                <div class="col-4">
                  <p class="text-center bg-dark text-white">Precio</p>
                  <canvas id="chartPrecio"></canvas>
                </div>
              </div>
            </div>
            <div class="carousel-item">
              <div class="row">
                <div class="col-4">
                  <p class="text-center bg-dark text-white">Precio unitario</p>
                  <canvas id="chartPrecioUnitario"></canvas>
                </div>
                <div class="col-4">
                  <p class="text-center bg-dark text-white">Volumen mercado</p>
                  <canvas id="chartVolMercado"></canvas>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

