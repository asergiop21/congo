<div id="alerts"></div>
<div class="bg-blue flex-dashboard">
  <div class="container-fluid">
    <div class="row pt-1">
      <div class="col-md-6 map-box">
        <div id="map_flex"></div>
      </div>
      <div class="col-md-6" id="intro">
        <div class="d-flex justify-content-center align-items-center bg-1 h-100">
          <div class="container">
            <p class="text-center">Seleccione en el mapa</p>
          </div>
        </div>
      </div>
      <%= render 'flex/dashboards/filters' %>
      <div class="col-md-12 table-box">
        <div class="mt-1">
          <div class="container-fluid mh-726">
            <%= render 'flex/dashboards/table' %>
            <%= render "flex/dashboards/new" %>
            <div class="mb-5">
              <button class="btn btn-secondary" onclick="genCharts();">Generar</button>
              <button class="btn btn-success" id="exportToExcel">Descargar</button>
              <button onclick="clearTable();" class="btn btn-danger">Limpiar</button>
            </div>
          </div>
        </div>
      </div>
    </div>
    <%= render 'flex/dashboards/charts' %>
  </div>
</div>

