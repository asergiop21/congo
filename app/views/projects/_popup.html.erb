  <div class="modal-dialog modal-lg" role="document">
    <div class="modal-content text-light">
      
      <div class="modal-header">
        <div class="list-group list-group-horizontal col-md-10 offset-md-1" id="list-tab" role="tablist">
          <a class="list-group-item list-group-item-action active" id="list-info-list" data-toggle="list" href="#list-info-projects" role="tab" aria-controls="info">Detalle Proyecto</a>
          <a class="list-group-item list-group-item-action" id="list-offer-list" data-toggle="list" href="#list-offer-projects" role="tab" aria-controls="offer">Oferta vs. Venta</a>
          <a class="list-group-item list-group-item-action" id="list-verif-list" data-toggle="list" href="#list-verif-projects" role="tab" aria-controls="verif">Verificación Información</a>
          <a class="list-group-item list-group-item-action" id="list-equip-list" data-toggle="list" href="#list-equip-projects" role="tab" aria-controls="equip">Puntos de Interés</a>
        </div>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>

      <div class="modal-body modal-overflow">
        <div class="tab-content" id="nav-tabContent">
          <div class="tab-pane fade show active" id="list-info-projects" role="tabpanel" aria-labelledby="list-info-list">
            <div id="popup_info_projects"></div>
          </div>
          <div class="tab-pane fade" id="list-offer-projects" role="tabpanel" aria-labelledby="list-offer-list">
            <div class="offset-md-2 col-md-8" id="offer_mix_container">
              <canvas id="offer_mix_canvas"></canvas>
            </div>
            <div class="offset-md-2 col-md-8" id="sale_mix_container">
              <canvas id="sale_mix_canvas"></canvas>
            </div>
          </div>
          <div class="tab-pane fade" id="list-verif-projects" role="tabpanel" aria-labelledby="list-verif-list">
            <%=simple_form_for :feedback, :url =>{:controller => :feedbacks, :action => :create}, remote: :true do |f|  %>
              <%= render  "projects/feedback", a: f %>
              <div class="text-center">
                <%= f.submit class: 'btn btn-primary', value: "Enviar Feedback" %>
              </div>
            <% end %>
          </div>
          <div class="tab-pane fade" id="list-equip-projects" role="tabpanel" aria-labelledby="list-equip-list">
            <div id="popup_equip_projects_feature_info"></div>
          </div>
        </div>
      </div>

      <div class="modal-footer">
        <button type="button" class="btn btn-primary" data-dismiss="modal">Cerrar</button>
      </div>

    </div>
  </div>
